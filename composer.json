{
    "name": "webregul/laravel-sms-ru-channel",
    "description": "Laravel sms.ru channel",
    "type": "library",
    "keywords": ["sms.ru", "laravel"],
    "homepage": "https://github.com/WebRegul/laravel-sms-ru-channel",
    "require": {
        "php": "^8.0",
        "webregul/sms-ru-client": "^2.0",
        "illuminate/notifications": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "WebRegul\\SmsRuChannel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebRegul\\SmsRuChannel\\Tests\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "webregul",
            "email": "info@webregul.ru",
            "role": "Developer",
            "homepage": "https://github.com/WebRegul"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit --do-not-cache-result --color=always --coverage-clover=coverage.xml"
    }
}
